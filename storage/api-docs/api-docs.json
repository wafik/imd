                    },
                    "waktu_imd": {
                        "type": "string",
                        "enum": [
                            "15 menit",
                            "30 menit",
                            "45 menit",
                            "60 menit"
                        ],
                        "example": "30 menit"
                    },
                    "nama_petugas": {
                        "type": "string",
                        "example": "Bidan Sarah"
