                                        },
                                        "data": {
                                            "properties": {
                                                "avatar_url": {
                                                    "type": "string",
                                                    "example": "https://example.com/storage/avatars/avatar.jpg"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/imds": {
            "get": {
                "tags": [
                    "IMD"
                ],
                "summary": "Get list of IMD records with filters",
                "operationId": "489a40dc47e0c1422f908009fbd9f65a",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page number",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Number of items per page",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 10
                        }
                    },
                    {
                        "name": "search",
                        "in": "query",
                        "description": "Search in patient name, RM number, or staff name",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "John"
                        }
                    },
                    {
                        "name": "cara_persalinan",
                        "in": "query",
                        "description": "Filter by delivery method",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "SC",
                                "Spontan"
                            ]
                        }
                    },
                    {
                        "name": "waktu_imd",
                        "in": "query",
                        "description": "Filter by IMD duration",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "15 menit",
                                "30 menit",
                                "45 menit",
                                "60 menit"
                            ]
                        }
                    },
                    {
                        "name": "tanggal_lahir_dari",
                        "in": "query",
                        "description": "Filter by birth date from",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date",
                            "example": "2023-01-01"
                        }
                    },
                    {
                        "name": "tanggal_lahir_sampai",
                        "in": "query",
                        "description": "Filter by birth date to",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date",
                            "example": "2023-12-31"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "IMD records retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Data IMD berhasil diambil"
                                        },
                                        "data": {
                                            "properties": {
                                                "data": {
                                                    "type": "array",
                                                    "items": {
                                                        "$ref": "#/components/schemas/Imd"
                                                    }
                                                },
                                                "current_page": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "last_page": {
                                                    "type": "integer",
                                                    "example": 5
                                                },
                                                "per_page": {
                                                    "type": "integer",
                                                    "example": 10
                                                },
                                                "total": {
                                                    "type": "integer",
                                                    "example": 50
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "IMD"
                ],
                "summary": "Create new IMD record",
                "operationId": "4e46308f8d440a912878c2e8a2f5783a",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ImdRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "IMD record created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Data IMD berhasil ditambahkan"
                                        },
                                        "data": {
                                            "$ref": "#/components/schemas/Imd"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
