                    },
                    "username": {
                        "type": "string",
                        "example": "johndoe"
                    },
                    "phone": {
                        "type": "string",
                        "example": "081234567890"
                    },
                    "avatar": {
                        "type": "string",
                        "example": "avatars/avatar.jpg",
                        "nullable": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            },
