                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            },
            "ImdRequest": {
                "required": [
                    "nama_pasien",
                    "alamat",
                    "no_rm",
                    "tanggal_lahir",
                    "cara_persalinan",
                    "tanggal_imd",
                    "waktu_imd",
                    "nama_petugas"
                ],
                "properties": {
                    "nama_pasien": {
                        "type": "string",
                        "example": "Siti Aminah"
                    },
                    "alamat": {
                        "type": "string",
                        "example": "Jl. Merdeka No. 123, Jakarta"
                    },
                    "no_rm": {
                        "type": "string",
                        "example": "RM001234"
                    },
                    "tanggal_lahir": {
                        "type": "string",
                        "format": "date",
                        "example": "1990-05-15"
                    },
                    "cara_persalinan": {
                        "type": "string",
                        "enum": [
                            "SC",
                            "Spontan"
                        ],
                        "example": "Spontan"
                    },
                    "tanggal_imd": {
                        "type": "string",
                        "format": "date",
